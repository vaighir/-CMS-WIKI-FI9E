              <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_xjVnkNOCEeu6VfbTNL_Jcg" name="get" specification="_xCe9QNOCEeu6VfbTNL_Jcg">
                <ownedParameter xmi:id="_xjVnkdOCEeu6VfbTNL_Jcg" name="email">
                  <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/UMLPrimitiveTypes.library.uml#String"/>
                </ownedParameter>
                <ownedParameter xmi:id="_xjVnktOCEeu6VfbTNL_Jcg" type="_xEg9kNOCEeu6VfbTNL_Jcg" isOrdered="true" direction="return">
                  <upperValue xmi:type="uml:LiteralUnlimitedNatural" xmi:id="_xjVnk9OCEeu6VfbTNL_Jcg" value="*"/>
                </ownedParameter>
                <language>java</language>
                <body>SessionFactory factory = new Configuration().configure(&quot;hibernate.cfg.xml&quot;).buildSessionFactory();&#xD;
		Session session = factory.getCurrentSession();&#xD;
&#xD;
		List&lt;User> users = new ArrayList&lt;User>();&#xD;
&#xD;
		try {&#xD;
			session.beginTransaction();&#xD;
&#xD;
			CriteriaBuilder builder = session.getCriteriaBuilder();&#xD;
			CriteriaQuery&lt;User> crit = builder.createQuery(User.class);&#xD;
			&#xD;
			Root&lt;User> root = crit.from(User.class);&#xD;
			crit.select(root);&#xD;
			crit.where( builder.equal( root.get(&quot;email&quot;), email.toLowerCase() ) );&#xD;
			&#xD;
			users = session.createQuery( crit ).getResultList();&#xD;
&#xD;
			session.getTransaction().commit();&#xD;
		} catch (Exception e) {&#xD;
			e.printStackTrace();&#xD;
		} finally {&#xD;
			if (session != null &amp;&amp; session.isOpen()) {&#xD;
				session.close();&#xD;
				factory.close();&#xD;
			}&#xD;
		}&#xD;
&#xD;
		return users;&#xD;
	</body>
              </ownedBehavior>
